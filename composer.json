{
    "name": "astrogoat/mailchimp",
    "description": "A Mailchimp app for Strata",
    "keywords": [
        "Astrogoat",
        "laravel",
        "mailchimp"
    ],
    "homepage": "https://github.com/astrogoat/mailchimp",
    "license": "MIT",
    "authors": [
        {
            "name": "Kristoffer Tonning",
            "email": "tonning@gmail.com",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.hlx.dev"
        }
    ],
    "require": {
        "php": "^8.0",
        "astrogoat/strata": "^0.3.0",
        "illuminate/contracts": "^8.37 || ^9.9",
        "mailchimp/marketing": "^3.0",
        "spatie/laravel-package-tools": "^1.4.3"
    },
    "require-dev": {
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "pestphp/pest": "^1.18",
        "pestphp/pest-plugin-laravel": "^1.1",
        "spatie/laravel-ray": "^1.23",
        "vimeo/psalm": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "Astrogoat\\Mailchimp\\": "src",
            "Astrogoat\\Mailchimp\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Astrogoat\\Mailchimp\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "./vendor/bin/pest --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Astrogoat\\Mailchimp\\MailchimpServiceProvider"
            ],
            "aliases": {
                "Mailchimp": "Astrogoat\\Mailchimp\\Facades\\Mailchimp"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
